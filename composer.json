{
    "name": "metadrop/drupal-dev",
    "description": "Development dependencies for Drupal projects on top of drupal/core-dev",
    "type": "metapackage",
    "authors": [
        {
            "name": "Omar",
            "email": "omar.lopesino@metadrop.net"
        }
    ],
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "require": {
        "bex/behat-step-time-logger": "^2.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "drupal/core-dev": "^9 || ^10",
        "drupal/devel": "^5.0 || ^4.0",
        "edgedesign/phpqa": "^1.23",
        "metadrop/behat-contexts": "^1.0",
        "metadrop/scripthor": "^1.0 || ^2",
        "phpspec/prophecy-phpunit": "^2 || ^1",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "drupal/behat_javascript": "^1.0",
        "metadrop/drupal-updater": "^1.0",
        "phpstan/phpstan": "1.10.x-dev",
        "phpstan/extension-installer": "1.2.x-dev",
        "mglaman/phpstan-drupal": "377.x-dev",
        "phpstan/phpstan-deprecation-rules": "1.1.x-dev",
